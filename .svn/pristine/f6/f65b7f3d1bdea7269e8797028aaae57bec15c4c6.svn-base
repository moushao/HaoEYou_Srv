<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical"
    tools:context="com.haoeyou.user.activity.FillArchivesActivity">

    <include
        android:id="@+id/toolbar_layout"
        layout="@layout/toolbar_layout"
        />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout

            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout

                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#fdFcFe"
                android:orientation="horizontal"
                android:visibility="gone">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="32dp"
                    android:gravity="center_vertical"
                    android:paddingLeft="10dp"
                    android:text="请仔细填写"
                    android:textColor="#a5bace"
                    android:textSize="13sp"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="32dp"
                    android:gravity="center_vertical"
                    android:text="就诊人"
                    android:textColor="#3eb0ff"
                    android:textSize="13sp"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="32dp"
                    android:gravity="center_vertical"
                    android:paddingRight="10dp"
                    android:text="资料"
                    android:textColor="#a5bace"
                    android:textSize="13sp"/>
            </LinearLayout>

            <LinearLayout

                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:paddingLeft="10dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:paddingBottom="12dp"
                    android:drawableLeft="@drawable/icon_person_info"
                    android:drawablePadding="10dp"
                    android:paddingRight="10dp"
                    android:paddingTop="16dp"
                    android:text="个人资料"
                    android:textColor="#3eb0ff"
                    android:textSize="13sp"/>
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="#d6e2f5"/>

            <LinearLayout

                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:paddingLeft="10dp">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:paddingBottom="12dp"
                    android:paddingRight="10dp"
                    android:paddingTop="16dp"
                    android:text="姓名"
                    android:textColor="#000000"
                    android:textSize="16sp"/>

                <EditText
                    android:id="@+id/name"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:layout_weight="1.5"
                    android:background="@null"
                    android:gravity="center_vertical"
                    android:hint="请填写真实姓名"
                    android:paddingBottom="12dp"
                    android:paddingRight="10dp"
                    android:paddingTop="16dp"
                    android:textColor="#000000"
                    android:textColorHint="#b9c9e1"
                    android:textSize="16sp"/>
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:background="#d6e2f5"/>

            <LinearLayout

                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:paddingLeft="10dp">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:paddingBottom="12dp"
                    android:paddingRight="10dp"
                    android:paddingTop="16dp"
                    android:text="性别"
                    android:textColor="#000000"
                    android:textSize="16sp"/>

                <TextView
                    android:id="@+id/sex"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:layout_weight="1.5"
                    android:background="@null"
                    android:gravity="center_vertical"
                    android:hint="请选性别"
                    android:paddingBottom="12dp"
                    android:paddingRight="10dp"
                    android:paddingTop="16dp"
                    android:textColor="#000000"
                    android:textColorHint="#b9c9e1"
                    android:textSize="16sp"/>
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:background="#d6e2f5"/>

            <LinearLayout

                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:paddingLeft="10dp">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:paddingBottom="12dp"
                    android:paddingRight="10dp"
                    android:paddingTop="16dp"
                    android:text="出生年月"
                    android:textColor="#000000"
                    android:textSize="16sp"/>

                <TextView
                    android:id="@+id/birthday"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:layout_weight="1.5"
                    android:background="@null"
                    android:gravity="center_vertical"
                    android:hint="请选择出生年月"
                    android:paddingBottom="12dp"
                    android:paddingRight="10dp"
                    android:paddingTop="16dp"
                    android:textColor="#000000"
                    android:textColorHint="#b9c9e1"
                    android:textSize="16sp"/>
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:background="#d6e2f5"/>

            <LinearLayout

                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:paddingLeft="10dp">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:paddingBottom="12dp"
                    android:paddingRight="10dp"
                    android:paddingTop="16dp"
                    android:text="年龄"
                    android:textColor="#000000"
                    android:textSize="16sp"/>

                <TextView
                    android:id="@+id/old"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:layout_weight="1.5"
                    android:background="@null"
                    android:gravity="center_vertical"
                    android:paddingBottom="12dp"
                    android:paddingRight="10dp"
                    android:paddingTop="16dp"
                    android:textColor="#000000"
                    android:textColorHint="#b9c9e1"
                    android:textSize="16sp"/>
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:background="#d6e2f5"/>

            <LinearLayout

                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:paddingLeft="10dp">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:paddingBottom="12dp"
                    android:paddingRight="10dp"
                    android:paddingTop="16dp"
                    android:text="联系电话"
                    android:textColor="#000000"
                    android:textSize="16sp"/>

                <EditText
                    android:id="@+id/tel"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:layout_weight="1.5"
                    android:background="@null"
                    android:gravity="center_vertical"
                    android:hint="请输入联系电话"
                    android:inputType="phone"
                    android:maxLength="15"
                    android:paddingBottom="12dp"
                    android:paddingRight="10dp"
                    android:paddingTop="16dp"
                    android:textColor="#000000"
                    android:textColorHint="#b9c9e1"
                    android:textSize="16sp"/>
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:background="#d6e2f5"/>

            <LinearLayout

                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:paddingLeft="10dp">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:paddingBottom="12dp"
                    android:paddingRight="10dp"
                    android:paddingTop="16dp"
                    android:text="紧急人联系电话:"
                    android:textColor="#000000"
                    android:textSize="16sp"/>

                <EditText
                    android:id="@+id/urgent_phone"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:layout_weight="1.5"
                    android:background="@null"
                    android:gravity="center_vertical"
                    android:hint="请输入联系电话"
                    android:paddingBottom="12dp"
                    android:paddingRight="10dp"
                    android:paddingTop="16dp"
                    android:textColor="#000000"
                    android:textColorHint="#b9c9e1"
                    android:textSize="16sp"/>
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:background="#d6e2f5"/>

            <LinearLayout

                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:paddingLeft="10dp">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:paddingBottom="12dp"
                    android:paddingRight="10dp"
                    android:paddingTop="16dp"
                    android:text="所在城市"
                    android:textColor="#000000"
                    android:textSize="16sp"/>

                <TextView
                    android:id="@+id/area"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:layout_weight="1.5"
                    android:background="@null"
                    android:gravity="center_vertical"
                    android:hint="请选择所在地址"
                    android:paddingBottom="12dp"
                    android:paddingRight="10dp"
                    android:paddingTop="16dp"
                    android:textColor="#000000"
                    android:textColorHint="#b9c9e1"
                    android:textSize="16sp"/>
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:background="#d6e2f5"/>


            <View
                android:layout_width="match_parent"
                android:layout_height="12dp"
                android:background="#fbfcfe"/>

            <LinearLayout

                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:paddingLeft="10dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:paddingBottom="12dp"
                    android:paddingRight="10dp"
                    android:drawablePadding="10dp"
                    android:drawableLeft="@drawable/icon_health"
                    android:paddingTop="16dp"
                    android:text="健康信息"
                    android:textColor="#3eb0ff"
                    android:textSize="13sp"/>
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="#d6e2f5"/>

            <LinearLayout

                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:paddingLeft="10dp">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:paddingBottom="12dp"
                    android:paddingRight="10dp"
                    android:paddingTop="16dp"
                    android:text="是否锻炼"
                    android:textColor="#000000"
                    android:textSize="16sp"/>

                <TextView
                    android:id="@+id/is_exercise"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:layout_weight="1.5"
                    android:background="@null"
                    android:gravity="center_vertical"
                    android:hint="请选择"
                    android:paddingBottom="12dp"
                    android:paddingRight="10dp"
                    android:paddingTop="16dp"
                    android:textColor="#000000"
                    android:textColorHint="#b9c9e1"
                    android:textSize="16sp"/>
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:background="#d6e2f5"/>

            <LinearLayout

                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:paddingLeft="10dp">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:paddingBottom="12dp"
                    android:paddingRight="10dp"
                    android:paddingTop="16dp"
                    android:text="是否吸烟"
                    android:textColor="#000000"
                    android:textSize="16sp"/>

                <TextView
                    android:id="@+id/is_smoking"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:layout_weight="1.5"
                    android:background="@null"
                    android:gravity="center_vertical"
                    android:hint="请选择"
                    android:paddingBottom="12dp"
                    android:paddingRight="10dp"
                    android:paddingTop="16dp"
                    android:textColor="#000000"
                    android:textColorHint="#b9c9e1"
                    android:textSize="16sp"/>
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:background="#d6e2f5"/>

            <LinearLayout

                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:paddingLeft="10dp">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:paddingBottom="12dp"
                    android:paddingRight="10dp"
                    android:paddingTop="16dp"
                    android:text="是否喝酒"
                    android:textColor="#000000"
                    android:textSize="16sp"/>

                <TextView
                    android:id="@+id/is_drinking"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:layout_weight="1.5"
                    android:background="@null"
                    android:gravity="center_vertical"
                    android:hint="请选择   "
                    android:paddingBottom="12dp"
                    android:paddingTop="16dp"
                    android:textColor="#000000"
                    android:textColorHint="#b9c9e1"
                    android:textSize="16sp"/>
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:background="#d6e2f5"/>

            <LinearLayout

                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:paddingLeft="10dp">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:paddingBottom="12dp"
                    android:paddingRight="10dp"
                    android:paddingTop="16dp"
                    android:text="家族遗传史"
                    android:textColor="#000000"
                    android:textSize="16sp"/>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1.5"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/genetic_history"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:layout_weight="1.5"
                        android:background="@null"
                        android:gravity="center_vertical"
                        android:hint="请编辑"
                        android:paddingBottom="12dp"
                        android:paddingRight="10dp"
                        android:paddingTop="16dp"
                        android:textColor="#000000"
                        android:textColorHint="#b9c9e1"
                        android:textSize="16sp"/>

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:visibility="invisible"
                        android:layout_marginRight="20dp"/>
                </LinearLayout>
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:background="#d6e2f5"/>

            <LinearLayout

                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:paddingLeft="10dp">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:paddingBottom="12dp"
                    android:paddingRight="10dp"
                    android:paddingTop="16dp"
                    android:text="药物过敏史"
                    android:textColor="#000000"
                    android:textSize="16sp"/>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1.5"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/allergy"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:layout_weight="1.5"
                        android:background="@null"
                        android:gravity="center_vertical"
                        android:hint="请编辑"
                        android:paddingBottom="12dp"
                        android:paddingRight="10dp"
                        android:paddingTop="16dp"
                        android:textColor="#000000"
                        android:textColorHint="#b9c9e1"
                        android:textSize="16sp"/>

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:visibility="invisible"
                        android:layout_marginRight="20dp"/>
                </LinearLayout>
            </LinearLayout>


            <View
                android:layout_width="match_parent"
                android:layout_height="12dp"
                android:background="#fbfcfe"/>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="#d6e2f5"/>

            <LinearLayout

                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:paddingLeft="10dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:drawableLeft="@drawable/icon_care_departmen"
                    android:drawablePadding="10dp"
                    android:gravity="center_vertical"
                    android:paddingBottom="12dp"
                    android:paddingRight="10dp"
                    android:paddingTop="16dp"
                    android:text="关注科室"
                    android:textColor="#3eb0ff"
                    android:textSize="13sp"/>
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="#d6e2f5"/>

            <LinearLayout

                android:layout_width="match_parent"
                android:layout_height="48dp"
                android:paddingLeft="10dp">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:paddingBottom="12dp"
                    android:paddingRight="10dp"
                    android:paddingTop="16dp"
                    android:text="关注科室(选填)"
                    android:textColor="#000000"
                    android:textSize="16sp"/>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1.5"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/department"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:layout_weight="1"
                        android:background="@null"
                        android:gravity="center_vertical"
                        android:hint="请选择馆关注科室"
                        android:paddingBottom="12dp"
                        android:paddingRight="10dp"
                        android:paddingTop="16dp"
                        android:textColor="#000000"
                        android:textColorHint="#b9c9e1"
                        android:textSize="16sp"/>

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:visibility="invisible"
                        android:layout_marginRight="20dp"/>
                </LinearLayout>
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginBottom="40dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:background="#d6e2f5"/>

            <include
                layout="@layout/layout_button"
                />
        </LinearLayout>
    </ScrollView>
</LinearLayout>
